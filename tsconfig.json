{
  "compilerOptions": {
    "jsx": "preserve",
    "module": "ESNext",
    "target": "es2020",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "paths": {
      "@/*": ["./app/frontend/*"],
      "~/*": ["./app/frontend/*"]
    }
  }
}
